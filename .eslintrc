{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-boolean-value": 0,
    "semi": [1, "always"],
    "indent": [2, "tab"],
    "no-tabs": 0,
    "react/jsx-indent": [2, "tab"],
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"]
  }
}
